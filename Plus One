class Solution {
public:
    vector<int> plusOne(vector<int>& digits) {
        int len=digits.size(),c=1;
        
        for(int i=len-1;i>=0;i--)
        {
            if(++digits[i]==10)
            digits[i] = 0;
            else
            return digits;
        }
        
        
        digits.insert(digits.begin(),1);
        
        return digits;
        
        
    }
};
